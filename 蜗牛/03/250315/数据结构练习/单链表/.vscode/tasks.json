{
	"version": "2.0.0",
	"tasks": [
	  {
		"type": "cppbuild",
		"label": "C Build",
		"command": "gcc",
		"args": [
		  "-std=c11",
		  "-Wall",
		  "-g",
		  "${file}",
		  "-o",
		  "${fileDirname}/${fileBasenameNoExtension}"
		],
		"options": {
		  "cwd": "${fileDirname}"
		},
		"group": {
		  "kind": "build",
		  "isDefault": true
		}
	  }
	]
  }